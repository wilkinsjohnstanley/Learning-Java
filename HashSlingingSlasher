import java.util.*;
public class HashSlingingSlasher {

	public static void main(String[] args) {
		HashMap<String, Integer>happy=new HashMap<String, Integer>();
		happy.put("a", 10);
		happy.put("b", 5);
		happy.put("c", 15);
		happy.remove("c", 15);
		happy.replace("b", 20);
		System.out.println(happy);

	}

}
//{a=10, b=5, c=15}
